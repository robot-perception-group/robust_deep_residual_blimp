<xml>
    <object name="ROSBridgeStatus" singleinstance="true" settings="false" category="System" priority="true">
        <description>ROSBridge Status Information</description>
       
	 
        <field name="TxPackets" units="count" type="uint32" elements="1"/>
        <field name="RxPackets" units="count" type="uint32" elements="1"/>
        <field name="TxFail" units="count" type="uint32" elements="1"/>
        <field name="RxFail" units="count" type="uint32" elements="1"/>
        <field name="PingRoundTripTime" units="seconds" type="float" elements="1"/>
        
        <access gcs="readwrite" flight="readwrite"/>
        <telemetrygcs acked="false" updatemode="manual" period="0"/>
        <telemetryflight acked="false" updatemode="periodic" period="5000"/>
        <logging updatemode="manual" period="0"/>
    </object>
</xml>
